  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "node ./zip-script.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production node dist/main",
