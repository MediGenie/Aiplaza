    "nanoid": "^3.3.4",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-apple": "^2.0.1",
    "passport-appleid": "^1.0.5",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
